        <area fill="#050008" />
      </rule>
      <rule e="way" k="*" v="apron">
        <area fill="#050008" />
      </rule>-->
      <!-- Airport passenger building -->
      <rule e="way" k="*" v="terminal|hangar">
        <use-area name="building" />
        <use-line name="building" />
        </rule> 
    </rule>

